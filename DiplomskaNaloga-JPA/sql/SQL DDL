create table profesor (
id int not null auto_increment,
imePriimek varchar(40) not null unique,
PRIMARY KEY (id)
);

create table program (
id int not null unique auto_increment,
code varchar(5) not null unique,
vrsta varchar(10) not null,
PRIMARY KEY (id)
);

create table zavod (
id int not null unique auto_increment,
code varchar(5) not null,
zavod varchar(40) not null,
PRIMARY KEY (id)
);

create table vrstastudija (
id int not null unique auto_increment,
code varchar(5) not null,
vrsta varchar(20) not null,
PRIMARY KEY (id)
);

create table leto (
id int not null auto_increment,
datum date not null,
PRIMARY KEY (id)
);

CREATE TABLE `diplomskadela` (
  id int NOT null auto_increment,
  priimekIme varchar(30) NOT NULL,
  naslov varchar(255) NOT NULL,
  datum date not null,
  id_profesor int NOT NULL,
  id_zavod varchar(10) not null,
  id_vrstastudija int not null,
  id_program varchar(5)  not null,
  PRIMARY KEY (`id`),
  FOREIGN KEY (id_profesor) REFERENCES profesor(id),
  FOREIGN KEY (id_zavod) REFERENCES zavod(id),
  FOREIGN KEY (id_vrstastudija) REFERENCES vrstastudija(id),
  FOREIGN KEY (id_program) REFERENCES program(id)
);